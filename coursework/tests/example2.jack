class Main {

    /** Sums up 1 + 2 + 3 + ... + m */
    function int sum (int/**/n) {
        var int sum, i;
        let i = 1;
        while (~(i > n)) {
            let sum = sum + i;
            let i = i + 1;
        }
        return sum;
    }

